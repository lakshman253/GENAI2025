name="lakshman" #string
age= 23 #int
price=29.99 #floating value

print(name)
print(age)
print(price)

print("My Name is :", name)
print("My age is :", age)
print("the price of the book is: ", price)

# '=' is an assignment operator

age2= age
print(age2)

print(type(name))
print(type(age))
print(type(price))
      
# INT can be positive , negative and zero value

Total = -389
print(type(Total))

